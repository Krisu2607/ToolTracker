<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tooltracker.controller.BackFromSharpenController">

    <GridPane alignment="CENTER" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml">
        <padding>
            <Insets bottom="20" left="20" right="20" top="20" />
        </padding>



        <Label text="Indeks narzędzia" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <TextField fx:id="indexTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label text="D1" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <TextField fx:id="D1textfield" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Label text="D2" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <TextField fx:id="D2textfield" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <Label text="L1" GridPane.columnIndex="0" GridPane.rowIndex="4" />
        <TextField fx:id="L1textfield" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <Label text="L2" GridPane.columnIndex="0" GridPane.rowIndex="5" />
        <TextField fx:id="L2textfield" GridPane.columnIndex="1" GridPane.rowIndex="5" />
        <VBox alignment="CENTER" spacing="10" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="10">
            <Button fx:id="confirmButton" onAction="#handleConfirmButton" text="Zatwierdź" />
            <Button fx:id="cancelButton" onAction="#handleCancelButton" text="Anuluj" />
        </VBox>

    </GridPane>


</AnchorPane>
